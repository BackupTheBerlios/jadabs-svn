<?xml version="1.0" encoding="ISO-8859-1"?>

<project>

	<extend>${basedir}/../../../project.xml</extend>
   
	<name>jadabs-im-testgui</name>	<!-- The full name of the project. This value is used when generating JavaDoc titles. -->	
	<id>jadabs-im-testgui</id>	<!-- The short name of the project. This value is used when naming jars and distribution files. -->		
	<groupId>jadabs-im</groupId>	<!-- The short name of the project group. This value is used to group all jars for a project in one directory. -->
	<artifactId>testgui</artifactId>	<!-- The unique id for an artifact produced by the project group, e.g. jboss-boot -->
	<package>ch.ethz.jadabs_im.testgui</package>

	<shortDescription>mservices-GUI for test</shortDescription>

	<properties>
		<osgi.import.service>ch.ethz.jadabs.remotefw.FrameworkManager</osgi.import.service>
	</properties>

   <dependencies>
   
        <dependency>
            <groupId>osgi</groupId>
            <artifactId>osgi-framework</artifactId>
            <version>1.2</version>
        </dependency>
        
        <dependency>
            <groupId>swt</groupId>
            <artifactId>swt</artifactId>
            <version>3.0.1-ppc</version>
        </dependency>
        
        <dependency>
            <groupId>jadabs</groupId>
            <artifactId>remotefw-api</artifactId>
            <version>${pom.currentVersion}</version>
        </dependency>
        
        <dependency>
            <groupId>jadabs</groupId>
            <artifactId>remotefw-impl</artifactId>
            <version>${pom.currentVersion}</version>
        </dependency>
	    
        <dependency>
            <groupId>jadabs</groupId>
            <artifactId>log4j-cdc</artifactId>
            <version>${pom.currentVersion}</version>
        </dependency>
        
        <dependency>
      		<groupId>jadabs</groupId>
      		<artifactId>jxme-services-api</artifactId>
      		<version>${pom.currentVersion}</version>
		</dependency>
		
		<dependency>
      		<groupId>jadabs</groupId>
      		<artifactId>sip-cons</artifactId>
      		<version>${pom.currentVersion}</version>
		</dependency>		
      
        <dependency>
      		<groupId>jadabs-im-api</groupId>
      		<artifactId>messenger-api</artifactId>
      		<version>${pom.currentVersion}</version>
		</dependency>
		
		<dependency>
      		<groupId>jadabs-im-jxme</groupId>
      		<artifactId>messenger-jxme</artifactId>
      		<version>${pom.currentVersion}</version>
		</dependency>
		
	    
	</dependencies>
  
  	<build>
		<resources>
	     
	     	<resource>
	        	<directory>resources/</directory>
	            <includes>
	                <include>logo_jadabs.png</include>
	            </includes>
	        </resource>
		 
	     </resources>
     </build>
     
</project>
